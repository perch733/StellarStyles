{
  "name": "stellarstyles.css",
  "version": "1.1.1",
  "description": "Alternative to normalize.css",
  "main": "dist/Stellarstyles.min.css",
  "exports": {
    ".": {
      "default": "./dist/Stellarstyles.min.css"
    },
    "./h1-legacy": {
      "default": "./dist/stellarstyles-h1-legacy.css"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "author": "Percy Chuzon <contacto@percychuzon.com>",
  "license": "ISC",
  "scripts": {
    "build": "postcss src/StellarStyles.css -o dist/Stellarstyles.min.css --no-map",
    "build:legacy": "postcss src/StellarStyles-h1-legacy.css -o dist/stellarstyles-h1-legacy.css --no-map",
    "build:all": "npm run build && npm run build:legacy"
  },
  "keywords": [
    "normalize.css",
    "CSS resets",
    "standardize.css"
  ],
  "homepage": "https://stellarstyles-css.percychuzon.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perch33/StellarStyles.git"
  },
  "bugs": {
    "url": "https://github.com/perch33/StellarStyles/issues"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1"
  }
}
