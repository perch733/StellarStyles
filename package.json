{
  "name": "stellarstyles.css",
  "version": "1.1.7",
  "description": "Alternative to normalize.css",
  "main": "dist/stellarstyles.min.css",
  "exports": {
    ".": {
      "default": "./dist/stellarstyles.min.css"
    },
    "./selection": {
      "default": "./dist/selection.min.css"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE.md"
  ],
  "author": "Percy Chuzon <contacto@percychuzon.com>",
  "license": "MIT",
  "scripts": {
    "build": "postcss src/stellarStyles.css -o dist/stellarstyles.min.css --no-map",
    "build:selection": "postcss src/selection.css -o dist/selection.min.css --no-map",
    "build:all": "npm run build && npm run build:selection"
  },
  "keywords": [
    "normalize.css",
    "reset",
    "css-reset",
    "CSS resets",
    "reset.css",
    "normalize",
    "standardize.css",
    "modern-normalize",
    "sanitize.css",
    "minimal-css",
    "css-baseline",
    "typography",
    "default-styles",
    "design-system",
    "postcss",
    "starter-styles",
    "frontend",
    "ui-framework",
    "cross-browser"
  ],
  "homepage": "https://stellarstyles.percychuzon.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perch733/StellarStyles.git"
  },
  "bugs": {
    "url": "https://github.com/perch733/StellarStyles/issues"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "cssnano": "^7.1.2",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1"
  }
}
